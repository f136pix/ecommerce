{
  "name": "scandiweb/fullstack",
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "App\\Domain\\": "src/Domain/"
    },
    "psr-0": {
      "": "src/"
    }
  },
  "require": {
    "nikic/fast-route": "^1.3",
    "webonyx/graphql-php": "^14.11.10",
    "doctrine/orm": "^2.14",
    "doctrine/dbal": "^3.9.3",
    "doctrine/persistence": "^2.5.7",
    "vlucas/phpdotenv": "^5.6",
    "ecodev/graphql-doctrine": "8.1.0",
    "symfony/cache": "^6.4",
    "doctrine/migrations": "^3.8",
    "ext-pdo": "*",
    "php-di/php-di": "^7.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0"
  },
  "scripts": {
    "cs-check": "phpcs --standard=PSR12 src",
    "cs-fix": "phpcbf --standard=PSR12 src"
  }
}
